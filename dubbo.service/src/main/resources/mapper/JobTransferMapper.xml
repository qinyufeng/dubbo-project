<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qyf.dubbowechat.mapper.JobTransferMapper">

	<!-- 开启二级缓存 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.qyf.dubbowechat.entity.JobTransfer">
		<id column="numid" property="numid" />
		<result column="applyuser" property="applyuser" />
		<result column="applydate" property="applydate" />
		<result column="name" property="name" />
		<result column="deptid" property="deptid" />
		<result column="reason" property="reason" />
		<result column="type" property="type" />
		<result column="startdeptid" property="startdeptid" />
		<result column="enddeptid" property="enddeptid" />
		<result column="startleader" property="startleader" />
		<result column="endleader" property="endleader" />
		<result column="startsalary" property="startsalary" />
		<result column="endsalary" property="endsalary" />
		<result column="state" property="state" />
		<result column="executedate" property="executedate" />
		<result column="processid" property="processid" />
		<result column="processname" property="processname" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        numid, applyuser, applydate, name, deptid, reason, type, startdeptid, enddeptid, startleader, endleader, startsalary, endsalary, state, executedate, processid, processname
    </sql>

</mapper>
